{
  "name": "apollo-subgraph-vapor",
  "version": "1.0.0",
  "description": "Apollo subgraph boilerplate with Prisma & TypeGraphQL",
  "keywords": [
    "prisma",
    "graphql",
    "apollo-server",
    "apollo-gateway",
    "apollo-subgraph",
    "typegraphql-prisma"
  ],
  "author": "salman-codewar",
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts .",
    "start": "ts-node src/index.ts",
    "migrate": "prisma migrate dev",
    "lint:fix": "eslint --fix --ext .ts .",
    "prettier-format": "prettier --write --config .prettierrc './src/**'",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.7.2",
    "@apollo/subgraph": "^2.4.7",
    "@prisma/client": "4.15.0",
    "class-validator": "^0.14.0",
    "eslint": "^8.42.0",
    "graphql": "latest",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "1.22.1",
    "graphql-tag": "^2.12.6",
    "reflect-metadata": "0.1.13",
    "type-graphql": "2.0.0-beta.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prisma": "4.15.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typegraphql-prisma": "^0.25.1",
    "typescript": "5.1.3"
  }
}
